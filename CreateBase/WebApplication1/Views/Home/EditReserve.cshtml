@model ReserveWebApp.Models.ReserveViewModel

<div class="text-right">
    <h1>Edit Reserve</h1>
</div>
<div class="row">
<div class="col-md-4">
    <form asp-controller="Home" asp-action="EditReserve" method="post">
        <div class="mb-3">
            <label asp-for="SelectedUserId"></label>
            <select asp-for="SelectedUserId" asp-items="Model.Users" class="custom-select"></select>
        </div>
        <div class="mb-3">
            <label asp-for="SelectedRoomId"></label>
            <select asp-for="SelectedRoomId" asp-items="Model.Rooms" class="custom-select"></select>
        </div>
        <div class="mb-3">
            <label asp-for="StartTime"></label>
            <input asp-for="StartTime" class="form-control"/>
        </div>
        <div class="mb-3">
            <label asp-for="EndTime"></label>
            <input asp-for="EndTime" class="form-control"/>
        </div>
        <div class="mb-3">
            <button type="submit" class="btn btn-primary">Edit Reserve</button>
        </div>
        <input asp-for="Id"/>
    </form>
</div>
</div>