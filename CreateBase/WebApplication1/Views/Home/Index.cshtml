@model List<WebApplication1.Controllers.ReservesViewModel>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-right">
    <h1>Reserves<br></h1>
</div>
<div class="text-left">
    @foreach (var reserve in Model)
    {
        <a asp-controller="Home" asp-action="EditReserve" asp-route-id="@reserve.Id">@reserve.Title</a><br/>
    }
</div>
